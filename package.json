{
	"name": "obsidian-move-file-plugin",
	"version": "0.1.0-beta.2",
	"description": "A plugin for Obsidian that allows you to move file and all the related files (inlinks) in the file tree to a new folder.",
	"main": "main.js",
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && cp manifest.json dist/",
		"changelog": "pnpm release-it --ci --no-git.push",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "biome check",
		"lint:fix": "biome check --fix",
		"test": "vitest",
		"prepare": "husky"
	},
	"keywords": ["obsidian", "plugin", "related move", "move files"],
	"author": "xjiaxiang_86@163.com",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@types/node": "^22.7.5",
		"builtin-modules": "4.0.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"memfs": "^4.14.0",
		"obsidian": "latest",
		"release-it": "^17.11.0",
		"tslib": "^2.8.1",
		"typescript": "5.6.3",
		"vite": "6.0.2",
		"vitest": "^2.1.3"
	},
	"dependencies": {}
}
